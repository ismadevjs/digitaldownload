<!DOCTYPE html>
<html lang="en">

    <head>
        <meta charset="utf-8" />
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=0, minimal-ui">
        <title><%=settings.sitename%> - Admin Panel</title>
        <meta content="Admin Dashboard" name="description" />
        <meta content="Themesbrand" name="author" />
        <link rel="shortcut icon" <% if(settings.logos.filename) { %>
            <img href="/<%=settings.logos.filename%>" alt="logo">
            <% } else { %>
             <img href="/fonts/eiefy_icons/logo.svg" alt="logo">
            <% } %>

        <!--Chartist Chart CSS -->
        <link rel="stylesheet" href="/assets/plugins/chartist/css/chartist.min.css">

        <link href="/assets/css/bootstrap.min.css" rel="stylesheet" type="text/css">
        <link href="/assets/css/metismenu.min.css" rel="stylesheet" type="text/css">
        <link href="/assets/css/icons.css" rel="stylesheet" type="text/css">
        <link href="/assets/css/style.css" rel="stylesheet" type="text/css">
        <link rel="stylesheet" href="/assets/plugins/summernote/summernote-bs4.css">

        <!--Jquery steps CSS -->
        <link rel="stylesheet" href="/assets/plugins/jquery-steps/jquery.steps.css">

        <!-- Dropzone css -->
        <link href="/assets/plugins/dropzone/dist/dropzone.css" rel="stylesheet" type="text/css">
    </head>

    <body>

        <!-- Begin page -->
        <div id="wrapper">

            <!-- Top Bar Start -->
            <div class="topbar">

                <!-- LOGO -->
                <div class="topbar-left">
                    <a href="/admin" class="logo">
                        <span>
                        <% if(settings.logos != null) {%>

                         
                         <img src="/<%=settings.logos.filename%>" alt="" height="18">
                            </span>
                        <i>
                                <img src="<%=settings.logos.filename%>" alt="" height="22">
                            </i>
                        <% } else { %>
                            <img src="/assets/images/logo-light.png" alt="" height="18">
                        </span>
                    <i>
                            <img src="/assets/images/logo-sm.png" alt="" height="22">
                        </i>
                        <% }%>
                    </a>
                </div>

                <nav class="navbar-custom">
                    <ul class="navbar-right list-inline float-right mb-0">
                       

                       

                        <!-- full screen -->
                        <li class="dropdown notification-list list-inline-item d-none d-md-inline-block">
                            <a class="nav-link waves-effect" href="#" id="btn-fullscreen">
                                <i class="mdi mdi-fullscreen noti-icon"></i>
                            </a>
                        </li>

                        
                        <li class="dropdown notification-list list-inline-item">
                            <div class="dropdown notification-list nav-pro-img">
                                <a class="dropdown-toggle nav-link arrow-none waves-effect nav-user" data-toggle="dropdown" href="#" role="button" aria-haspopup="false" aria-expanded="false">
                                    <img src="/images/user.png" alt="user" class="rounded-circle">
                                </a>
                                <div class="dropdown-menu dropdown-menu-right profile-dropdown ">
                                    <!-- item-->
                                    <a class="dropdown-item d-block" href="/admin/settings"><i class="mdi mdi-settings m-r-5"></i> Settings</a>
                                    <div class="dropdown-divider"></div>
                                    <a class="dropdown-item text-danger" href="/admin/logout"><i class="mdi mdi-power text-danger"></i> Logout</a>
                                </div>
                            </div>
                        </li>

                    </ul>

                    <ul class="list-inline menu-left mb-0">
                        <li class="float-left">
                            <button class="button-menu-mobile open-left waves-effect">
                                <i class="mdi mdi-menu"></i>
                            </button>
                        </li>
                        <li class="d-none d-sm-block">
                            <div class="dropdown pt-3 d-inline-block">
                                <a class="btn btn-light dropdown-toggle" href="#" role="button" id="dropdownMenuLink" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                        Create
                                    </a>

                                <div class="dropdown-menu" aria-labelledby="dropdownMenuLink">
                                    <a class="dropdown-item" href="/admin/categories">Category</a>
                                    <a class="dropdown-item" href="/admin/pages">Page</a>
                                    <a class="dropdown-item" href="/admin/items">Product</a>
                                    <div class="dropdown-divider"></div>
                                    <a class="dropdown-item" href="/admin/whatWeDo">What we do</a>
                                </div>
                            </div>
                        </li>
                    </ul>

                </nav>

            </div>
            <!-- Top Bar End -->

            <!-- ========== Left Sidebar Start ========== -->
            <div class="left side-menu">
                <div class="slimscroll-menu" id="remove-scroll">

                    <!--- Sidemenu -->
                    <div id="sidebar-menu">
                        <!-- Left Menu Start -->
                        <ul class="metismenu" id="side-menu">
                            <li class="menu-title">Main</li>
                            <li>
                                <a href="/admin" class="waves-effect">
                                    <i class="ti-home"></i> <span> Dashboard </span>
                                </a>
                            </li>
                            <li>
                                <a href="/admin/categories" class="waves-effect"><i class="mdi mdi-file-tree"></i><span> Categories </span></a>
                            </li>
                            <li>
                                <a href="/admin/items" class="waves-effect"><i class="mdi mdi-cart-arrow-right"></i><span> Products </span></a>
                            </li>
                            <li>
                                <a href="/admin/free_item" class="waves-effect"><i class="mdi mdi-cart-arrow-right"></i><span> Free Item </span></a>
                            </li>
                            <li>
                                <a href="/admin/customers" class="waves-effect"><i class="ti-user"></i><span> Customers </span></a>
                            </li>
                            <li>
                                <a href="/admin/ourteam" class="waves-effect"><i class="fab fa-teamspeak"></i><span> Our Team </span></a>
                            </li>
                            <li>
                                <a href="/admin/downloads" class="waves-effect"><i class=" mdi mdi-cloud-download"></i><span> Downloads </span></a>
                            </li>
                            <li>
                                <a href="/admin/testimonial" class="waves-effect"><i class="mdi mdi-notification-clear-all"></i><span> Testimonial </span></a>
                            </li>
                            <li>
                                <a href="/admin/pages" class="waves-effect"><i class="fas fa-file-alt"></i><span> Pages </span></a>
                            </li>
                            <li>
                                <a href="/admin/settings" class="waves-effect"><i class="mdi mdi-message-settings-variant"></i><span> Settings </span></a>
                            </li>
                            <li>
                                <a href="/admin/services" class="waves-effect"><i class="fab fa-servicestack"></i><span> Services </span></a>
                            </li>
                           
                            <li>
                                <a href="/admin/whatWeDo" class="waves-effect"><i class="fad fa-save"></i><span> What We Do </span></a>
                            </li>
                            
                            <li>
                                <a href="/admin/socialNetwork" class="waves-effect"><i class="fas fa-share-alt"></i><span> Social Network </span></a>
                            </li>
                            
                            
                            
                            



                        </ul>

                    </div>
                    <!-- Sidebar -->
                    <div class="clearfix"></div>

                </div>
                <!-- Sidebar -left -->

            </div>
            <!-- Left Sidebar End -->
