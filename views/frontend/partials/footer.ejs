
        <!-- FOOTER -->
        <footer>
            <div class="container">
                <div class="row">
                    <div class="col-md-4 col-sm-6 col-xs-6">
                        <a href="/" class="logo">
                            <img src="/<%=settings.logos.filename%>" alt="logo">
                        </a>
                        <p><p><%=settings.settingsAbout%></p></p>
                    </div>
                    <div class="col-md-4 col-sm-6 col-xs-6">
                        <ul class="footer-menu">
                            <li>
                                <a href="#" class="border-decor">Store</a>
                                <ul>
                                    <%catsLimite.forEach(function(category) {%>
                                        <li><a href="/browse/<%=category._id%>"><%=category.categoryName%></a></li>
                                    <%})%>
                                   
                                </ul>
                            </li>
                            <li>
                                <a href="#" class="border-decor">Info</a>
                                <ul>
                                    <% pages.forEach(function(page) { %>
                                     <li><a href="/pages/<%=page._id%>"><%=page.pageTitle%></a></li> 
                                    <% }) %>
                                   
                                </ul>
                            </li>
                        </ul>
                    </div>
                    <div class="clearfix hidden-md hidden-lg"></div>
                    <div class="col-md-4 col-sm-12">
                        <p>Subscribe to our newsletter to receive news, updates, free stuff and new releases by email.</p>
                        <form action="/" method="post">
                            <input type="text" class="form-control" name="emailMailChimp" placeholder="Email Address" />
                            <input type="submit" class="btn btn-small btn-gray" value="Subscribe" />
                        </form>
                        <ul class="icons">
                            <li><a href="//facebook.com/<%=socials.facebook%>"><i class="fa fa-facebook-square" aria-hidden="true"></i></a></li>
                            <li><a href="//twitter.com/<%=socials.twitter%>"><i class="fa fa-twitter" aria-hidden="true"></i></a></li>
                            <li><a href="//plus.google.com/<%=socials.google%><i class="fa fa-google-plus" aria-hidden="true"></i></a></li>
                            <li><a href="//dribbble.com/<%=socials.dribble%>"><i class="fa fa-dribbble" aria-hidden="true"></i></a></li>
                            <li><a href="//www.behance.net/<%=socials.behance%>"><i class="fa fa-behance" aria-hidden="true"></i></a></li>
                        </ul>
                    </div>
                    <div class="col-md-12">
                        <div class="copyright pull-right"><%=settings.footer%></div>
                    </div>
                </div>
            </div>
        </footer>

        <script src="/external/jquery/jquery.js"></script>
        <script src="/external/bootstrap/bootstrap.min.js"></script>
        <script src="/external/slick/slick.min.js"></script>
        <script src="/external/app.js"></script>
