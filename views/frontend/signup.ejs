<%-include('partials/header')%>
        <!-- MAIN -->
        <main class="signup">
            <div class="container">
                <section class="text-block mb-90">
                    <h4 class="text-center">Get started for Free</h4>
                    <% errors.forEach(function(e) { %>
                        <div class="alert alert-success text-center" role="alert">
                            <%= e %>
                        </div>
                    <%  })%>
                    <p class="text-center border-decor bd-center"></p>
                </section>
                <section class="image text-center mb-50">
                    <img src="fonts/eiefy_icons/Signup.svg" alt="">
                </section>
                <section class="login-form text-center">
                    <form action="/ok" method="POST" class="mb-10">
                        <div class="row">
                            <div class="col-md-6 col-sm-6">
                                <input type="text" name="name" class="form-control" placeholder="Full Name">
                            </div>
                            <div class="col-md-6 col-sm-6">
                                <input type="email" name="email" class="form-control" placeholder="Email Address">
                            </div>
                            <div class="col-md-6 col-sm-6">
                                <input type="password" name="password" class="form-control" placeholder="Password">
                            </div>
                            <div class="col-md-6 col-sm-6">
                                <div class="ipt-btn-block">
                                    <input type="password" name="rpassword" class="form-control" placeholder="Re type Password">
                                    <input type="submit" class="btn btn-small" value="Sign up">
                                </div>
                            </div>
                        </div>
                    </form>
                    <a href="#" class="font-16 weight-lighter color-theme-hover">By signing up you agree to out <span class="color-dark">Terms of Service.</span></a>
                </section>
            </div>
        </main>

        <%-include('partials/footer')%>
